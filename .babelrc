{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "useBuiltIns": true
    }],
    "react",
    "stage-2"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel",
        "babel-plugin-root-import",
        "transform-class-properties",
        "transform-es2015-destructuring",
        "transform-object-rest-spread"
      ]
    }
  }
}
